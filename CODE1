#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char name[100];
    int age;
    float cgpa;
    char grade;
    scanf("%s",name);
    scanf("%d",&age);
    scanf("%f",&cgpa);
    scanf(" %c",&grade);
    
     cgpa = floor(cgpa * 100) / 100;
    
    printf("Name: %s\n",name);
    printf("Age: %d\n",age);
    printf("CGPA: %.2f\n",cgpa);
    printf("Grade: %c",grade);
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
