#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    
    int a,b,c;
    scanf("%d\n%d\n%d\n",&a,&b,&c);
    int sp=a*b;
    int rem=sp-(a*c);
    int profit=rem-100;
    printf("%d",profit);
    
    
    return 0;
}
